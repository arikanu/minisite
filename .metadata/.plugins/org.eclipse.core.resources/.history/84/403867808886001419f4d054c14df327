public class asdf {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Date date = new Date();
		Date dateAlesIb = new Date();
		
		long diffAlesIb = getDateDiff(dateAlesIb, date, TimeUnit.DAYS);
